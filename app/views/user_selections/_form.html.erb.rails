<div class="container">
  <div class="form-wrapper" style="width: 400px; margin: 0 auto;">
    <%= simple_form_for [@tech_stack, @user_selection] do |f| %>
    <%= f.input :tech_stack %>
    <%= f.input :experience %>
    <%= f.input :objective %>
    <%= f.button :submit, "Find your buddy", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
