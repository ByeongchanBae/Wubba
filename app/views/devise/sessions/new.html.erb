<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="login-image">
  <div class="login-page">
    <div class="login-form">
      <%= link_to "WUBBA", root_path, class: "login-wubba" %>
      <h5 style="margin-bottom: 20px"> Welcome back to Wubba! </h5>
      <form class="login-form">
        <%= f.input :email,
        required: false,
        autofocus: true,
        input_html: { autocomplete: "email" } %>
        <%= f.input :password,
        required: false,
        input_html: { autocomplete: "current-password" } %>
        <button>login</button>
        <p class="message">Not registered? <%= render "devise/shared/links" %></p>
      </form>
    </div>
  </div>
</div>
<% end %>
